{
  "name": "sugi-lab-vsc-tools",
  "displayName": "sugi-lab-vsc-tools",
  "description": "This is mini tools for vscode by sugimura lab.",
  "version": "0.0.3",
  "publisher": "Sugi-lab",
  "repository": "https://github.com/Hiroshi-Sugimura/sugi-lab-vsc-tools.git",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sugimode.upcaseForwardChar",
        "title": "Upcase Forward Char: Sugimode"
      },
      {
        "command": "sugimode.daytimeRenewal",
        "title": "Daytime Renewal: Sugimode"
      }
    ],
    "keybindings": [
      {
        "command": "sugimode.upcaseForwardChar",
        "when": "editorTextFocus",
        "key": "ctrl+u",
        "mac": "ctrl+u"
      },
      {
        "command": "sugimode.daytimeRenewal",
        "when": "editorTextFocus",
        "key": "ctrl+c d",
        "mac": "ctrl+c d"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.82.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6",
    "@vscode/test-electron": "^2.3.4"
  }
}